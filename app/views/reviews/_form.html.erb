<%= form_with(model: [@restaurant, Review.new], local: true ) do |form| %>
  <%= render 'errors' %>

  <div id="star-rating"></div>
  <div class="form-group">
    <%= form.label :comment %>
    <%= form.text_area :comment, class: "form-control"  %>
  </div>

  <div class="actions">
    <%= form.submit class: "btn btn-primary"%>
  </div>
<% end %>
  

